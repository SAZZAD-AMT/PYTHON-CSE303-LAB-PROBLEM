    "listing client printers sample": {
        "prefix": "jspm listing client printers sample",
        "body": [
			"<p>For more code samples please visit <a href=\"https://www.neodynamic.com/products/printing/js-print-manager/articles\" target=\"_black\">https://www.neodynamic.com/products/printing/js-print-manager/articles</a></p>",
            "<div style=\"text-align:center\">",
			"\t<h1>Installed Client Printers</h1>",
			"\t<hr />",
			"\t<select name=\"installedPrinterName\" id=\"installedPrinterName\"></select>",
			"</div>",
			"<!--JSPrintManager scripts-->",
            "<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/JSPrintManager.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/zip.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/zip-ext.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/deflate.js\"> </script>",
			"<!--end JSPrintManager scripts-->",
			"<!--JSPrintManager List Client Printers Sample-->",
			"<script type=\"text/javascript\">",
			"\t//WebSocket settings",
			"\tJSPM.JSPrintManager.auto_reconnect = true;",
			"\tJSPM.JSPrintManager.start();",
			"\tJSPM.JSPrintManager.WS.onStatusChanged = function () {",
			"\t\tif (jspmWSStatus()) {",
			"\t\t\t//get client installed printers",
			"\t\t\tJSPM.JSPrintManager.getPrinters().then(function (clientPrinters) {",
			"\t\t\t\tvar sel = document.getElementById(\"installedPrinterName\");",
			"\t\t\t\tfor (var i = 0; i < clientPrinters.length; i++) {",
			"\t\t\t\t\tvar option = document.createElement('option');",
			"\t\t\t\t\toption.value = option.text = clientPrinters[i];",
			"\t\t\t\t\tsel.add(option);",
			"\t\t\t\t}",
			"\t\t\t});",
			"\t\t}",
			"\t};",
			"\t//Check JSPM WebSocket status",
			"\tfunction jspmWSStatus() {",
			"\t\tif (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Open)",
			"\t\t\treturn true;",
			"\t\telse if (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Closed) {",
			"\t\t\talert('JSPrintManager (JSPM) is not installed or not running! Download JSPM Client App from https://neodynamic.com/downloads/jspm');",
			"\t\t\treturn false;",
			"\t\t}",
			"\t\telse if (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Blocked) {",
			"\t\t\talert('JSPM has Blocked this website!');",
			"\t\t\treturn false;",
			"\t\t}",
			"\t}",
			"</script>",
			"<!--end JSPrintManager List Client Printers Sample-->\n"
        ],
        "description": "A sample of how to get the list of installed printers from the client machine."
    },
    "pdf printing sample": {
        "prefix": "jspm pdf printing sample",
        "body": [
			"<p>For more code samples please visit <a href=\"https://www.neodynamic.com/products/printing/js-print-manager/articles\" target=\"_black\">https://www.neodynamic.com/products/printing/js-print-manager/articles</a></p>",
			"<div style=\"text-align:center\">",
			"\t<h1>Advanced PDF Printing from Javascript</h1>",
			"\t<hr />",
			"\t<div>",
			"\t\t<div>",
			"\t\t\t<label for=\"txtPdfFile\">PDF File URL:</label>",
			"\t\t\t<input type=\"text\" name=\"txtPdfFile\" id=\"txtPdfFile\" value=\"https://neodynamic.com/temp/LoremIpsum.pdf\" />",
			"\t\t</div>",
			"\t\t<div>",
			"\t\t\t<label for=\"lstPrinters\">Printers:</label>",
			"\t\t\t<select name=\"lstPrinters\" id=\"lstPrinters\" onchange=\"showSelectedPrinterInfo();\" ></select>",
			"\t\t</div>",
			"\t\t<div>",
			"\t\t\t<label for=\"lstPrinterTrays\">Supported Trays:</label>",
			"\t\t\t<select name=\"lstPrinterTrays\" id=\"lstPrinterTrays\" ></select>",
			"\t\t</div>",
			"\t\t<div>",
			"\t\t\t<label for=\"lstPrinterPapers\">Supported Papers:</label>",
			"\t\t\t<select name=\"lstPrinterPapers\" id=\"lstPrinterPapers\" ></select>",
			"\t\t</div>",
			"\t\t<div>",
			"\t\t\t<label for=\"lstPrintRotation\">Print Rotation (Clockwise):</label>",
			"\t\t\t<select name=\"lstPrintRotation\" id=\"lstPrintRotation\" >",
			"\t\t\t\t<option>None</option>",
			"\t\t\t\t<option>Rot90</option>",
			"\t\t\t\t<option>Rot180</option>",
			"\t\t\t\t<option>Rot270</option>",
			"\t\t\t</select>",
			"\t\t</div>",
			"\t</div>",
			"\t<div>",
			"\t\t<div>",
			"\t\t\t<label for=\"txtPagesRange\">Pages Range: [e.g. 1,2,3,10-15]</label>",
			"\t\t\t<input type=\"text\"  id=\"txtPagesRange\">",
			"\t\t</div>",
			"\t\t<div>",
			"\t\t\t<div >",
			"\t\t\t\t<label><input id=\"chkPrintInReverseOrder\" type=\"checkbox\" value=\"\">Print In Reverse Order?</label>",
			"\t\t\t</div>",
			"\t\t</div>",
			"\t\t<div>",
			"\t\t\t<div >",
			"\t\t\t\t<label><input id=\"chkPrintAnnotations\" type=\"checkbox\" value=\"\">Print Annotations? <span><em>Windows Only</em></span></label>",
			"\t\t\t</div>",
			"\t\t</div>",
			"\t\t<div>",
			"\t\t\t<div >",
			"\t\t\t\t<label><input id=\"chkPrintAsGrayscale\" type=\"checkbox\" value=\"\">Print As Grayscale? <span><em>Windows Only</em></span></label>",
			"\t\t\t</div>",
			"\t\t</div>",
			"\t</div>",
			"\t<hr />",
			"\t<button onclick=\"print();\">Print Now...</button>",
			"</div>",
			"<!--JSPrintManager scripts-->",
            "<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/JSPrintManager.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/zip.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/zip-ext.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/deflate.js\"> </script>",
			"<!--end JSPrintManager scripts-->",
			"<!--JSPrintManager Advanced PDF Printing Sample-->",
            "<script type=\"text/javascript\">",
			"\n",
			"\tvar clientPrinters = null;",
			"\tvar _this = this;",
			"\n",
			"\t//WebSocket settings",
			"\tJSPM.JSPrintManager.auto_reconnect = true;",
			"\tJSPM.JSPrintManager.start();",
			"\tJSPM.JSPrintManager.WS.onStatusChanged = function () {",
			"\t\tif (jspmWSStatus()) {",
			"\t\t\t//get client installed printers",
			"\t\t\tJSPM.JSPrintManager.getPrintersInfo().then(function (printersList) {",
			"\t\t\t\tclientPrinters = printersList;",
			"\t\t\t\tvar sel = document.getElementById(\"lstPrinters\");",
			"\t\t\t\tfor (var i = 0; i < clientPrinters.length; i++) {",
			"\t\t\t\t\tvar option = document.createElement('option');",
			"\t\t\t\t\toption.value = option.text = clientPrinters[i].name;",
			"\t\t\t\t\tsel.add(option);",
			"\t\t\t\t}",
			"\t\t\t\t_this.showSelectedPrinterInfo();",
			"\t\t\t});",
			"\t\t}",
			"\t};",
			"\n",
			"\t//Check JSPM WebSocket status",
			"\tfunction jspmWSStatus() {",
			"\t\tif (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Open)",
			"\t\t\treturn true;",
			"\t\telse if (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Closed) {",
			"\t\t\talert('JSPrintManager (JSPM) is not installed or not running! Download JSPM Client App from https://neodynamic.com/downloads/jspm');",
			"\t\t\treturn false;",
			"\t\t}",
			"\t\telse if (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Blocked) {",
			"\t\t\talert('JSPM has Blocked this website!');",
			"\t\t\treturn false;",
			"\t\t}",
			"\t}",
			"\n",
			"\t//Do printing...",
			"\tfunction print() {",
			"\t\tif (jspmWSStatus()) {",
			"\n",
			"\t\t\t//Create a ClientPrintJob",
			"\t\t\tvar cpj = new JSPM.ClientPrintJob();",
			"\n",
			"\t\t\t//Set Printer info",
			"\t\t\tvar sel = document.getElementById(\"lstPrinters\");",
			"\t\t\tvar text = sel.options[sel.selectedIndex].text;",
			"\t\t\tvar myPrinter = new JSPM.InstalledPrinter(text);",
			"\n",
			"\t\t\tsel = document.getElementById(\"lstPrinterPapers\");",
			"\t\t\ttext = sel.options[sel.selectedIndex].text;",
			"\t\t\tmyPrinter.paperName = text;",
			"\n",
			"\t\t\tsel = document.getElementById(\"lstPrinterTrays\");",
			"\t\t\ttext = sel.options[sel.selectedIndex].text;",
			"\t\t\tmyPrinter.trayName = text;",
			"\n",
			"\t\t\tcpj.clientPrinter = myPrinter;",
			"\n",
			"\t\t\t//Set PDF file",
			"\t\t\tvar my_file = new JSPM.PrintFilePDF(document.getElementById(\"txtPdfFile\").value, JSPM.FileSourceType.URL, \"MyFile.pdf\", 1);",
			"\t\t\tsel = document.getElementById(\"lstPrintRotation\");",
			"\t\t\ttext = sel.options[sel.selectedIndex].text;",
			"\t\t\tmy_file.printRotation = JSPM.PrintRotation[text];",
			"\t\t\tmy_file.printRange = document.getElementById(\"txtPagesRange\").value;",
			"\t\t\tmy_file.printAnnotations = document.getElementById(\"chkPrintAnnotations\").checked;",
			"\t\t\tmy_file.printAsGrayscale = document.getElementById(\"chkPrintAsGrayscale\").checked;",
			"\t\t\tmy_file.printInReverseOrder = document.getElementById(\"chkPrintInReverseOrder\").checked;",
			"\n",
			"\t\t\tcpj.files.push(my_file);",
			"\n",
			"\t\t\t//Send print job to printer!",
			"\t\t\tcpj.sendToClient();",
			"\n",
			"\t\t}",
			"\t}",
			"\n",
			"\tfunction showSelectedPrinterInfo() {",
			"\t\t// get selected printer index",
			"\t\tvar idx = document.getElementById(\"lstPrinters\").selectedIndex;",
			"\t\t// get supported trays",
			"\t\tvar selTrays = document.getElementById(\"lstPrinterTrays\");",
			"\t\tselTrays.innerText = null;",
			"\t\tfor (var i = 0; i < clientPrinters[idx].trays.length; i++) {",
			"\t\t\tvar option = document.createElement('option');",
			"\t\t\toption.value = option.text = clientPrinters[idx].trays[i];",
			"\t\t\tselTrays.add(option);",
			"\t\t}",
			"\t\t// get supported papers",
			"\t\tvar selPapers = document.getElementById(\"lstPrinterPapers\");",
			"\t\tselPapers.innerText = null;",
			"\t\tfor (var i = 0; i < clientPrinters[idx].papers.length; i++) {",
			"\t\t\tvar option = document.createElement('option');",
			"\t\t\toption.value = option.text = clientPrinters[idx].papers[i];",
			"\t\t\tselPapers.add(option);",
			"\t\t}",
			"\t}",
			"\n",
			"</script>",
			"<!--end JSPrintManager Advanced PDF Printing Sample-->\n"
        ],
        "description": "Advanced PDF printing sample."
    },
    "zebra zpl printing sample": {
        "prefix": "jspm zebra zpl printing sample",
        "body": [
			"<p>For more code samples please visit <a href=\"https://www.neodynamic.com/products/printing/js-print-manager/articles\" target=\"_black\">https://www.neodynamic.com/products/printing/js-print-manager/articles</a></p>",
			"<div style=\"text-align:center\">",
			"\t<h1>Print Zebra ZPL commands from Javascript</h1>",
			"\t<hr />",
			"\t<label class=\"checkbox\">",
			"\t\t<input type=\"checkbox\" id=\"useDefaultPrinter\" /> <strong>Print to Default printer</strong>",
			"\t</label>",
			"\t<p>or...</p>",
			"\t<div id=\"installedPrinters\">",
			"\t\t<label for=\"installedPrinterName\">Select an installed Printer:</label>",
			"\t\t<select name=\"installedPrinterName\" id=\"installedPrinterName\"></select>",
			"\t</div>",
			"\t<br /><br />",
			"\t<button onclick=\"print();\">Print Now...</button>",
			"</div>",
			"\n",
			"<!--JSPrintManager scripts-->",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/JSPrintManager.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/zip.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/zip-ext.js\"> </script>",
			"<script type=\"text/javascript\" src=\"https://jsprintmanager.azurewebsites.net/scripts/deflate.js\"> </script>",
			"<!--end JSPrintManager scripts-->",
			"<!--JSPrintManager Zebra ZPL Printing Sample-->",
			"<script type=\"text/javascript\">",
			"\n",
			"\t//WebSocket settings",
			"\tJSPM.JSPrintManager.auto_reconnect = true;",
			"\tJSPM.JSPrintManager.start();",
			"\tJSPM.JSPrintManager.WS.onStatusChanged = function () {",
			"\t\tif (jspmWSStatus()) {",
			"\t\t\t//get client installed printers",
			"\t\t\tJSPM.JSPrintManager.getPrinters().then(function (clientPrinters) {",
			"\t\t\t\tvar sel = document.getElementById(\"installedPrinterName\");",
			"\t\t\t\tfor (var i = 0; i < clientPrinters.length; i++) {",
			"\t\t\t\t\tvar option = document.createElement(\"option\");",
			"\t\t\t\t\toption.value = option.text = clientPrinters[i];",
			"\t\t\t\t\tsel.add(option);",
			"\t\t\t\t}",
			"\t\t\t});",
			"\t\t}",
			"\t};",
			"\n",
			"\t//Check JSPM WebSocket status",
			"\tfunction jspmWSStatus() {",
			"\t\tif (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Open)",
			"\t\t\treturn true;",
			"\t\telse if (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Closed) {",
			"\t\t\talert('JSPrintManager (JSPM) is not installed or not running! Download JSPM Client App from https://neodynamic.com/downloads/jspm');",
			"\t\t\treturn false;",
			"\t\t}",
			"\t\telse if (JSPM.JSPrintManager.websocket_status == JSPM.WSStatus.Blocked) {",
			"\t\t\talert('JSPM has Blocked this website!');",
			"\t\t\treturn false;",
			"\t\t}",
			"\t}",
			"\n",
			"\t//Do printing...",
			"\tfunction print(o) {",
			"\t\tif (jspmWSStatus()) {",
			"\t\t\t//Create a ClientPrintJob",
			"\t\t\tvar cpj = new JSPM.ClientPrintJob();",
			"\t\t\t//Set Printer type (Refer to the help, there many of them!)",
			"\t\t\tif (document.getElementById(\"useDefaultPrinter\").checked) {",
			"\t\t\t\tcpj.clientPrinter = new JSPM.DefaultPrinter();",
			"\t\t\t} else {",
			"\t\t\t\tvar sel = document.getElementById(\"installedPrinterName\");",
			"\t\t\t\tvar text = sel.options[sel.selectedIndex].text;",
			"\t\t\t\tcpj.clientPrinter = new JSPM.InstalledPrinter(text);",
			"\t\t\t}",
			"\t\t\t//Set content to print...",
			"\t\t\t//Create Zebra ZPL commands for sample label",
			"\t\t\tvar cmds =  \"^XA\";",
			"\t\t\tcmds += \"^FO20,30^GB750,1100,4^FS\";",
			"\t\t\tcmds += \"^FO20,30^GB750,200,4^FS\";",
			"\t\t\tcmds += \"^FO20,30^GB750,400,4^FS\";",
			"\t\t\tcmds += \"^FO20,30^GB750,700,4^FS\";",
			"\t\t\tcmds += \"^FO20,226^GB325,204,4^FS\";",
			"\t\t\tcmds += \"^FO30,40^ADN,36,20^FDShip to:^FS\";",
			"\t\t\tcmds += \"^FO30,260^ADN,18,10^FDPart number #^FS\";",
			"\t\t\tcmds += \"^FO360,260^ADN,18,10^FDDescription:^FS\";",
			"\t\t\tcmds += \"^FO30,750^ADN,36,20^FDFrom:^FS\";",
			"\t\t\tcmds += \"^FO150,125^ADN,36,20^FDAcme Printing^FS\";",
			"\t\t\tcmds += \"^FO60,330^ADN,36,20^FD14042^FS\";",
			"\t\t\tcmds += \"^FO400,330^ADN,36,20^FDScrew^FS\";",
			"\t\t\tcmds += \"^FO70,480^BY4^B3N,,200^FD12345678^FS\";",
			"\t\t\tcmds += \"^FO150,800^ADN,36,20^FDMacks Fabricating^FS\";",
			"\t\t\tcmds += \"^XZ\";",
			"\t\t\tcpj.printerCommands = cmds;",
			"\t\t\t//Send print job to printer!",
			"\t\t\tcpj.sendToClient();",
			"\t\t}",
			"\t}",
			"\n",
			"</script>",
			"<!--end JSPrintManager Zebra ZPL Printing Sample-->\n"
        ],
        "description": "Zebra ZPL printing sample."
    }
	
}